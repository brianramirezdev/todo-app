{
    "name": "backend",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "dev-old": "nodemon --exec ts-node src/index.ts",
        "dev": "nodemon --exec ts-node -r tsconfig-paths/register src/index.ts",
        "build": "tsc",
        "start": "ts-node -r tsconfig-paths/register src/index.ts",
        "start:migrate": "npm run migration:run && npm start",
        "test": "cross-env NODE_ENV=test jest",
        "migration:generate": "cross-env NODE_OPTIONS=\"-r tsconfig-paths/register\" typeorm-ts-node-commonjs migration:generate -d src/config/typeorm.config.ts",
        "migration:run": "cross-env NODE_OPTIONS=\"-r tsconfig-paths/register\" typeorm-ts-node-commonjs migration:run -d src/config/typeorm.config.ts",
        "migration:revert": "cross-env NODE_OPTIONS=\"-r tsconfig-paths/register\" typeorm-ts-node-commonjs migration:revert -d src/config/typeorm.config.ts"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "type": "commonjs",
    "dependencies": {
        "cors": "^2.8.6",
        "cross-env": "^10.1.0",
        "dotenv": "^17.2.3",
        "express": "^5.2.1",
        "pg": "^8.17.2",
        "reflect-metadata": "^0.2.2",
        "typeorm": "^0.3.28"
    },
    "devDependencies": {
        "@types/cors": "^2.8.19",
        "@types/express": "^5.0.6",
        "@types/jest": "^30.0.0",
        "@types/node": "^25.0.10",
        "@types/supertest": "^6.0.3",
        "dotenv-cli": "^11.0.0",
        "jest": "^30.2.0",
        "nodemon": "^3.1.11",
        "supertest": "^7.2.2",
        "ts-jest": "^29.4.6",
        "ts-node": "^10.9.2",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.9.3"
    }
}
